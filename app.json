{
  "name": "spark-bot-seed",
  "description": "A spark bot starter template",
  "keywords": [
    "bot",
    "spark",
    "communication"
  ],
  "repository": "https://github.com/brh55/spark-bot-seed",
  "env": {
    "WEBHOOK_URL": {
      "description": "Spark web hook url in order for the bot to be notified",
      "value": "https://heroku-bot-name.herokuapp.com/",
      "required": true
    },
    "TOKEN": {
      "description": "Spark token for authentication",
      "required": true
    },
    "PORT": {
      "description": "Port for the bot to be running on",
      "required": false
    }
  }
}
