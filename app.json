{
  "name": "Spark Magic 8 Ball",
  "description": "A magic 8 ball in any spark room",
  "logo": "https://raw.githubusercontent.com/brh55/spark-magic-8-ball/master/bot-icon.png",
  "keywords": [
    "bot",
    "spark",
    "communication"
  ],
  "repository": "https://github.com/brh55/spark-bot-seed",
  "env": {
    "WEBHOOK_URL": {
      "description": "Spark web hook url in order for the bot to be notified",
      "value": "https://heroku-bot-name.herokuapp.com/flint",
      "required": true
    },
    "TOKEN": {
      "description": "Spark token for authentication",
      "required": true
    },
    "PORT": {
      "description": "Port for the bot to be running on",
      "required": false
    }
  }
}
